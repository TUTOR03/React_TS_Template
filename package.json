{
  "name": "react-ts-template",
  "private": true,
  "version": "1.0.0",
  "author": {
    "name": "TUTOR03",
    "email": "stukalov.dev@gmail.com"
  },
  "license": "MIT",
  "engines": {
    "node": ">=18.16 <19",
    "yarn": ">=1.21.1 <=1.22.19"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development vite",
    "build": "cross-env NODE_ENV=production rm -rf ./dist && vite build",
    "preview": "vite preview",
    "lint": "yarn lint:js && yarn lint:css",
    "lint:js": "yarn eslint --ext .ts,tsx --cache --cache-strategy content ./src",
    "lint:css": "stylelint \"./src/**/*.css\"",
    "format": "yarn format:js && yarn format:css",
    "format:js": "yarn eslint --ext .ts,tsx --fix --cache --cache-strategy content ./src",
    "format:css": "stylelint \"./src/**/*.css\" --fix",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "prepare": "husky install"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.0.7",
    "@storybook/addon-interactions": "^7.0.7",
    "@storybook/addon-links": "^7.0.7",
    "@storybook/blocks": "^7.0.7",
    "@storybook/builder-vite": "^7.0.7",
    "@storybook/react": "^7.0.7",
    "@storybook/react-vite": "^7.0.7",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.1",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.14",
    "cross-env": "^7.0.3",
    "cssnano": "^6.0.0",
    "cssnano-preset-lite": "^3.0.0",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-storybook": "^0.6.11",
    "husky": "^8.0.0",
    "postcss": "^8.4.23",
    "postcss-cli": "^10.1.0",
    "postcss-url": "^10.1.3",
    "prettier": "^2.8.8",
    "prop-types": "^15.8.1",
    "storybook": "^7.0.7",
    "stylelint": "^15.6.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-order": "^6.0.3",
    "typescript": "^5.0.4",
    "vite": "^4.3.2",
    "vite-plugin-checker": "^0.5.6",
    "vite-plugin-mkcert": "^1.14.0",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.30.1"
  }
}
